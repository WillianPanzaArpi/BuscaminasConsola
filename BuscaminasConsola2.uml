          <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_SGvdsLaxEe-NO4GSsf5MxQ" name="main" specification="_SELR4LaxEe-NO4GSsf5MxQ">
            <ownedParameter xmi:id="_SGvdsbaxEe-NO4GSsf5MxQ" name="args">
              <type xmi:type="uml:PrimitiveType" href="pathmap://UML_LIBRARIES/UMLPrimitiveTypes.library.uml#String"/>
