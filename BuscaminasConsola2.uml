          <ownedParameter xmi:id="_SG3ZhLaxEe-NO4GSsf5MxQ" type="_SEXfILaxEe-NO4GSsf5MxQ" direction="return"/>
          <language>java</language>
          <body>this.filas = filas;&#xD;
     this.columnas = columnas;&#xD;
     this.minas = minas;&#xD;
     inicializarTablero(); // Inicializar la matriz de celdas&#xD;
     colocarMinas();       // Colocar minas aleatoriamente en el tablero&#xD;
